{
  "name": "@pijma/react-native",
  "description": "Wrapper around react-native",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/mjs/index.mjs",
  "types": "./lib/dts/index.d.ts",
  "react-native": "./lib/jsx/index.js",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/mjs/index.mjs",
      "require": "./lib/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma-native.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma-native/issues"
  },
  "homepage": "https://github.com/qiwi/pijma-native/#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "concurrently 'npm:build:*'",
    "build:mjs": "tsc --project tsconfig.mjs.json && tsc-esm-fix --tsconfig tsconfig.mjs.json --ext .mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-esm-fix --target lib/cjs --ext .cjs",
    "build:jsx": "tsc --project tsconfig.jsx.json",
    "build:dts": "tsc --project tsconfig.dts.json",
    "style": "eslint --fix src examples && prettier --loglevel silent --write src examples"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-native": ">=0.50.0",
    "tslib": ">=2.0.0"
  },
  "devDependencies": {
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/react-native": "0.67.4",
    "concurrently": "7.1.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-qiwi": "1.16.1",
    "prettier": "2.6.2",
    "prettier-config-qiwi": "1.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "tsc-esm-fix": "2.12.4",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  },
  "packageManager": "yarn@3.2.0"
}
