{
  "name": "@pijma/kashka",
  "description": "Pijma Kashka",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/mjs/index.mjs",
  "types": "./lib/dts/index.d.ts",
  "react-native": "./lib/jsx/index.js",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/mjs/index.mjs",
      "require": "./lib/cjs/index.cjs"
    },
    "./package.json": "./package.json",
    "./loader.cjs": "./loader.cjs",
    "./transformer.cjs": "./transformer.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma-native.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma-native/issues"
  },
  "homepage": "https://qiwi.github.io/pijma-native/",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "README.md",
    "generator.cjs",
    "loader.cjs",
    "transformer.cjs",
    "find-up.cjs",
    "read-pkg-up.cjs"
  ],
  "scripts": {
    "clean": "yarn infra:clean",
    "build": "yarn infra:build",
    "build:mjs": "yarn infra:build:mjs",
    "build:cjs": "yarn infra:build:cjs",
    "build:tsc": "yarn infra:build:tsc",
    "style": "yarn infra:style"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@pijma/infra": "workspace:*",
    "@pijma/react-native": "workspace:*",
    "@pijma/react-native-svg": "workspace:*",
    "@react-navigation/bottom-tabs": "6.3.2",
    "@react-navigation/drawer": "6.4.3",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/native-stack": "6.7.0",
    "markdown-to-jsx": "7.1.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-live": "2.4.1",
    "react-native": "0.69.7",
    "react-native-gesture-handler": "2.8.0",
    "react-native-reanimated": "2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "3.18.2"
  },
  "dependencies": {
    "@qiwi/esm": "1.1.8",
    "@textlint/markdown-to-ast": "12.2.2",
    "fast-glob": "3.2.12",
    "find-up": "6.3.0",
    "react-docgen-typescript": "2.2.2",
    "read-pkg-up": "9.1.0",
    "require-from-string": "2.0.2"
  }
}
