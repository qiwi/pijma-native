{
  "name": "@pijma/react-native-svg",
  "description": "Wrapper around react-native-svg",
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/mjs/index.mjs",
  "types": "./lib/dts/index.d.ts",
  "react-native": "./lib/jsx/index.js",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/mjs/index.mjs",
      "require": "./lib/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma-native.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma-native/issues"
  },
  "homepage": "https://qiwi.github.io/pijma-native/",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "concurrently 'npm:build:*'",
    "build:mjs": "tsc --project tsconfig.mjs.json && tsc-esm-fix --tsconfig tsconfig.mjs.json --ext .mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-esm-fix --target lib/cjs --ext .cjs",
    "build:jsx": "tsc --project tsconfig.jsx.json",
    "build:dts": "tsc --project tsconfig.dts.json",
    "style": "eslint --fix src && prettier --loglevel silent --write src"
  },
  "devDependencies": {
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/react-native": "0.67.9",
    "concurrently": "7.2.2",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-qiwi": "1.17.1",
    "prettier": "2.7.1",
    "prettier-config-qiwi": "1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-svg": "12.3.0",
    "tsc-esm-fix": "2.18.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@types/react-native": "0.67.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-svg": "12.3.0",
    "tslib": "2.4.0"
  },
  "packageManager": "yarn@4.0.0-rc.9"
}
