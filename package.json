{
  "name": "pijma",
  "version": "2.0.0",
  "license": "MIT",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "source": "src/index.ts",
  "react-native": "src/index.ts",
  "files": [
    "src",
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib web/build",
    "build": "concurrently 'npm:build:*'",
    "build:check": "tsc --project tsconfig.build.json",
    "build:entry": "node esbuild.script",
    "build:types": "dts-bundle-generator --project tsconfig.build.json --no-check --no-banner --silent --out-file lib/index.d.ts src/index.ts",
    "build:guide": "styleguidist build --mode production",
    "start": "styleguidist server --mode development"
  },
  "dependencies": {
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@types/react-native": "^0.67.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.67.4",
    "react-native-svg": "^12.3.0",
    "react-native-web": "^0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.8",
    "babel-loader": "^8.2.4",
    "babel-plugin-extension-resolver": "^1.0.12",
    "babel-plugin-react-native-web": "^0.17.7",
    "concurrently": "^7.0.0",
    "dts-bundle-generator": "^6.5.0",
    "esbuild": "^0.14.27",
    "esbuild-loader": "^2.18.0",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-qiwi": "^1.16.1",
    "prettier": "^2.6.0",
    "prettier-config-qiwi": "^1.6.1",
    "react-docgen-typescript": "^2.2.2",
    "react-styleguidist": "^11.2.0",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "packageManager": "yarn@3.2.0"
}
