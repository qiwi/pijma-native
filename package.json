{
  "name": "pijma",
  "description": "Велосипед над react-native",
  "version": "2.4.1",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "source": "./src/index.ts",
  "react-native": "./src/index.ts",
  "main": "./lib/es5/index.js",
  "module": "./lib/es6/index.js",
  "types": "./lib/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/es6/index.js",
      "require": "./lib/es5/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib web/build",
    "build": "concurrently 'npm:build:*'",
    "build:es5": "tsc --project tsconfig.es5.json",
    "build:es6": "tsc --project tsconfig.es6.json",
    "build:dts": "tsc --project tsconfig.dts.json",
    "build:rsg": "styleguidist --config styleguide.config.cjs build --mode production",
    "start": "styleguidist --config styleguide.config.cjs server --mode development",
    "test": "exit 0",
    "style": "eslint --fix src && prettier --loglevel silent --write src",
    "examples": "node examples.script.cjs",
    "release": "semrel"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-native": ">=0.50.0",
    "react-native-svg": ">=12.0.0",
    "react-native-web": ">=0.17.0",
    "tslib": ">=2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@babel/runtime": "7.17.8",
    "@qiwi/semrel-toolkit": "3.2.8",
    "@react-navigation/bottom-tabs": "6.3.1",
    "@react-navigation/drawer": "6.4.1",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.1",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@types/react-native": "0.67.3",
    "babel-loader": "8.2.4",
    "babel-plugin-extension-resolver": "1.0.12",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-react-native-web": "0.17.7",
    "babel-plugin-tsconfig-paths-module-resolver": "1.0.3",
    "concurrently": "7.1.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-qiwi": "1.16.1",
    "markdown-to-jsx": "7.1.7",
    "metro-react-native-babel-preset": "0.70.0",
    "prettier": "2.6.2",
    "prettier-config-qiwi": "1.6.1",
    "react": "17.0.2",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.0",
    "react-native-gesture-handler": "2.3.2",
    "react-native-reanimated": "2.6.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "3.13.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-styleguidist": "11.2.0",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "webpack": "5.71.0"
  },
  "packageManager": "yarn@3.2.0"
}
